    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Tracker",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/cmd/tracker/main.go",
            "env": {
            },
            "args": [
                "-db-path","${workspaceFolder}/data/tracker.db",
                "-data-path", "${workspaceFolder}/data",
                "-config-file", "${workspaceFolder}/data/etc/tracker.yml",
                // "-video-device", "0",
                "-upload-server-addr", "trackerd-dev.uaptn.com:443",
                "-trackerd-server-addr", "trackerapi-dev.uaptn.com:443"
            ]
        }
    ]
}